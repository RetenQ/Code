package workIn20201031;

public class EasterSunday {

	public static void main(String[] args) {
		
		double y = 2020 ;
		//STEP1
		double s1 = y %19 ;
		//STEP2
		double  s2_1 = y / 100 ;
		double  s2_2 = y % 100 ; 
		//STEP3
		double s3_1 = s2_1 / 4 ;
		double s3_2 = s2_1 % 4 ;
		//STEP4
		double s4 = (s2_1 * 8 + 13) /25 ;
		//STEP5 
		double s5 = (s1 * 19 +s2_1-s3_1-s4 +15)%30 ; 
		
		
		
		
		
	}

}
