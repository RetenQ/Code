package workIn1115;

public class Song {

	public static void main(String[] args) {
		int count = 10;
		while (count>0) {
	 if(count==1) System.out.println("1 in a bed and the little one said,\n 'Alone at last'");	
	 else System.out.println(count +"in a bed and the little one said,\n "+"'Roll over, roll over'\n They all rolled over and one fell out,"  );
		count--;
			
		}
		
	}

}
